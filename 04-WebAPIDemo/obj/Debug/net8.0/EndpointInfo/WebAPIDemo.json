          },
          "productId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "Product": {
        "required": [
          "naam",
          "prijs"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "naam": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "beschrijving": {
            "maxLength": 500,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "prijs": {
            "maximum": 10000,
            "minimum": 0,01,
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      }
    }
